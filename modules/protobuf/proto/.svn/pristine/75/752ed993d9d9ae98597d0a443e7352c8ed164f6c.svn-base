// 英雄列表 :37 ====>服务器推送
message HeroListMsg {
	repeated HeroMsg list = 1;
}

// 单个英雄 :38 ====>服务器推送
message HeroMsg {
	required int32 heroId = 1; // 英雄Id
	required int32 level = 2; // 等级
	required string skillState = 3; //技能激活状态
	required int32 goldLevel = 4; //金身等级
}

// 英雄金身等级列表 :112 ====>服务器推送
message HeroGoldListMsg {
	repeated HeroGoldMsg list = 1;
}
message HeroGoldMsg {
	required int32 heroId = 1; // 英雄Id
	required int32 goldLevel = 2; //金身等级
}

