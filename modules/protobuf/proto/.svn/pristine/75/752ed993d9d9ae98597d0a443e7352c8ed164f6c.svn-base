// 装备列表 : 42 ====>服务器推送
message EquipListMsg {
	repeated EquipMsg list = 1;
}
// 装备 : 41 ====>服务器推送
message EquipMsg {
	required int32 id = 1;       // 唯一id
	required int32 equipId = 2; // 装备Id
	required int32 level = 3; // 等级
	required bool isWear = 4; // 是否佩戴
	required int32 latticeId = 5; // 格子id
	required string attributeId = 6; //属性列表Id
	required int32 quality = 7; //品质
	required string name = 8; //装备名称
	required int32 destroyNum = 9;// 分解获得多少核心
	required bool isNewEquip = 10; //是否是新装备
	required bool lock = 11; //是否锁定（true-锁定 false-解锁）
}
// 技能cd  44: ====>服务器推送
message SkillCdListMsg {
	repeated SkillCdMsg list = 1;
}
// 技能cd  45: ====>服务器推送
message SkillCdMsg {
	required int32 skillId = 1; // 技能Id
	required string endTime = 2; // 结束时间
	optional string effectEndTime = 3; //技能有效期结束时间
}

